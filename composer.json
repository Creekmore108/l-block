{
    "name": "creekmore108/l-block",
    "description": "A User can block another User",
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Creekmore",
            "email": "creekmore108@gmail.com",
            "homepage": "https://github.com/creekmore108"
        }
    ],
    "minimum-stability": "stable",
    "homepage": "https://github.com/creekmore108/l-block",
    "keywords": [
        "Laravel",
        "LBlock",
        "Block users"
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "php": "^8.1",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Creekmore108\\LBlock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creekmore108\\LBlock\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creekmore108\\LBlock\\LBlockServiceProvider"
            ],
            "aliases": {
                "LBlock": "Creekmore108\\LBlock\\Facades\\LBlock"
            }
        }
    }
}
